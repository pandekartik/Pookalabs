---
import Card from "./Card.astro";
const projects = [
    {
    heading: "Neutron AR/AP Solution",
    body: 'Logo, Branding and Digital Product Design',
    imageUrl: 'neutron',
    logoUrl : "9",
    projectUrl : "neutron",
  },
  {
    heading: "GuideMeDoc",
    body: 'UX/UI Design and Digital Product Design',
    imageUrl: 'guidemedoc',
    logoUrl : "guidemedoc",
    projectUrl : "guidemedoc",
  },
  {
    heading: "Logofolio",
    body: 'Some of our latest logo designs',
    imageUrl: 'logofolio',
    logoUrl : "",
    projectUrl : "logofolio",
  },
  {
    heading: "SPC Website",
    body: 'UX/UI Design & Web Development',
    imageUrl: 'spc',
    logoUrl : "8",
